<footer id="footer" class="l-footer">
    {% if breadCrumbs %}
        <div class="l-footer__breadcrumbs">
            {% include "layouts/breadcrumbs.njk" %}
        </div>
    {% endif %}
    <div class="l-footer__inner u-gutter--lg">
        <a href="#" class="l-footer__page-top" aria-label="Page Top">
            <svg width="16" height="16">
                <use xlink:href="#icon_arrow"></use>
            </svg>
        </a>
        <nav class="l-footer__nav">
            {% for nav in navigation %}
                <a class="l-footer__nav__item" href="{{ nav.url | url}}" >{{ nav.label }}</a>
            {% endfor %}
        </nav>
    </div>
</footer>