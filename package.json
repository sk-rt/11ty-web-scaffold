{
  "name": "11ty-handson",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "run-p serve:eleventy watch:webpack",
    "build": "run-s build:eleventy build:webpack",
    "serve:eleventy": "APP_ENV=local eleventy --serve",
    "build:eleventy": "APP_ENV=production eleventy",
    "watch:webpack": "webpack --watch",
    "build:webpack": "NODE_ENV=production webpack --mode production",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint src/js/**/*",
    "lint:prettier": "prettier --check src",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-p fix"
    }
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-img": "^1.0.0",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "add": "^2.0.6",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "dotenv": "^11.0.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^2.4.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "sass": "^1.47.0",
    "sass-loader": "^12.4.0",
    "svg-sprite-loader": "^6.0.11",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "yarn": "^1.22.17"
  },
  "dependencies": {
    "destyle.css": "^3.0.2",
    "smoothscroll-polyfill": "^0.4.4"
  }
}
